{
  "@context": {
    "@vocab": "https://schema.hbp.eu/graphQuery/",
    "query": "https://schema.hbp.eu/myQuery/",
    "fieldname": {
      "@id": "fieldname",
      "@type": "@id"
    },
    "merge": {
      "@type": "@id",
      "@id": "merge"
    },
    "relative_path": {
      "@id": "relative_path",
      "@type": "@id"
    }
  },
  "fields": [
    {
      "fieldname": "query:name",
      "relative_path": "http://schema.org/name",
	  "filter": {
		  "op": "starts_with",
		  "value": "Density measurements of different receptors"
	  }
    },
    {
      "fieldname": "query:owners",
      "relative_path": [
        "https://schema.hbp.eu/minds/owners",
        "http://schema.org/name"
      ],
	  "filter": {
        "op": "contains",
        "value": "Palomero"
      }
    },
    {
      "fieldname": "query:parcellationAtlas",
      "relative_path": "https://schema.hbp.eu/minds/parcellationAtlas",
	  "required": true,
	  "fields": [
        {
          "fieldname": "query:@id",
          "relative_path": "@id",
		  "filter": {
			  "op": "equals",
			  "value": "https://nexus.humanbrainproject.org/v0/data/minds/core/parcellationatlas/v1.0.0/94c1125b-b87e-45e4-901c-00daee7f2579"
		  }
		},
        {
          "fieldname": "query:name",
          "relative_path": "http://schema.org/name"
        }
      ]
    },
    {
      "fieldname": "query:parcellationRegion",
      "relative_path": "https://schema.hbp.eu/minds/parcellationRegion",
      "fields": [
        {
          "fieldname": "query:@id",
          "relative_path": "@id"
        },
        {
          "fieldname": "query:name",
          "relative_path": "http://schema.org/name"
        }
      ]
    },
	{
		"fieldname": "query:v1.0.0",
		"relative_path": [
			{
				"@id": "minds/core/fileassociation/v1.0.0",
				"reverse": true
			},
			"https://schema.hbp.eu/cscs/absolute_path"
		]
	}
  ]
}
